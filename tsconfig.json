{
  "compilerOptions": {
    "module": "amd",
    "target": "es2015",
    "strict": false,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "bookmarklet/dist",
    "lib": ["es2015", "dom"]
  },
  "include": ["bookmarklet/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
